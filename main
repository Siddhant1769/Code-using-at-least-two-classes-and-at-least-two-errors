#include <iostream>
using namespace std;

class Person {
public:
    string name;
    int age;

    void display() {
        cout << "Name: " << name << ", Age: " << age << endl;
    }
};

class Student : public Person {
public:
    string school;

    void displayInfo() {
        display(); // Calls Person's display
        cout << "School: " << school << endl;
    }
};

int main() {
    Student s;
    s.name = "Alice";
    s.age = 20;
    s.school = "Greenwood High";

    s.displayInfo();

    Person p;
    p.setName("Bob"); // ❌ Error 1: setName is not defined in Person

    Student s2;
    s2.grade = "A"; // ❌ Error 2: 'grade' is not a member of Student

    return 0;
}
